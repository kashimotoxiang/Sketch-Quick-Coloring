{
  "name": "Quick Note",
  "identifier": "com.sketchapp.quicknote",
  "version": "0.8",
  "description": "Make Annotation in Sketch with shortcuts",
  "authorEmail": "kashimotoxiang@outlook.com",
  "author": "kashimoto",
  "commands": [
