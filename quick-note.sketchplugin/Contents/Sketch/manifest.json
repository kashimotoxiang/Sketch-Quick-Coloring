      "identifier": "quick-coloring-lightblue"
    },
    {
      "script": "quick-color.js",
      "handler": "ColoringYellow",
      "shortcut": "ctrl 5",
      "name": "Yellow",
      "identifier": "quick-coloring-yellow"
    },
    {
      "script": "quick-color.js",
      "handler": "ColoringMagenta",
      "shortcut": "ctrl 6",
